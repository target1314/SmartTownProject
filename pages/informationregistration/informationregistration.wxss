/* pages/informationregistration/informationregistration.wxss */
.navigator-hover {
  color:blue;
}

.top-image{
  width: 100%;
  height: 150px;
}
.tellsection {
 margin: 16px;
}
.tellinput {
  border: 1px solid #efeff4;
  height: 25px;
  padding-top: 4px;
}

.title{
  color: red;
}
.title-text{
  color: #000;
  margin-left: 4px;
  size: 20px;
  line-height:40px;
}
.tellsection input {
  color: #000;
  width: 100%;
  padding-left: 5px;
  border-radius: 5px;
}

.navbar{
  flex: none;
  display: flex;
  background: #fff;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}
.navbar .item.active{
  color: #FFCC00;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #FFCC00;
}