/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.comm-btn {
  width: 90%;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  background-color: blue;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.item-hover {
  background: white;
}

@import "/styles/variables.wxss";
@import "/styles/nodata.wxss";
